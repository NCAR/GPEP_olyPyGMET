! printout for tmean
! added by tgq for diagnose
if (Z_reg(2) .eq. 42.90625 .and. Z_reg(3) .eq. -124.28125 .and. t .eq. 1) then

  print *, "Stop after one regression"
  print *, "time step is", t
  print *, "grid number is", g
  print *, "Z_reg is", Z_reg

  print *, "regression tmean(g, t) is ", tmean(g, t)
  print *, "close_loc_t(g, :) is ", close_loc_t(g, :)
  print *, "close_loc_t(g, :) is ", close_loc_t(g, :)
  print *, "y_tmean_red is ", y_tmean_red
  print *, "x_red_t is "

  do i = 1, (close_count_t(g)-1)
   print *, "x_red_t is", x_red_t(i,:)
  end do


  do i = 1, (close_count_t(g)-1)
   print *, "w_temp_red is", w_temp_red(i,i)
  end do

  print *, "twx_red is", twx_red
  print *, "close_weights_t(g, :) is", close_weights_t(g, :)

  print *, "close_meta 1-5: stnlat,stnlon,grdlat,grdlon,distance"
  print *, "close_meta_t"
  do i = 1, (close_count_t(g)-1)
   print *, close_meta_t(:,g,i)
  end do

  print *, 'b', b

  call EXIT(0)

end if

