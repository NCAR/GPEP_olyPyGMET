sbatch --array=2-40%39 Plato_readown.sh


for (( y=2017; y<=2017; y++))
do
for (( m=1; m<=12; m++ ))
do
if ((m<10)); then
sbatch Plato_"$y"0"$m".sh
else
sbatch Plato_"$y""$m".sh
fi
done
done


for (( y=2017; y<=2017; y++))
do
mv stndata_${y}* /datastore/GLOBALWATER/CommonData/EMDNA/PyGMETout_update
mv output_${y}* /datastore/GLOBALWATER/CommonData/EMDNA/PyGMETout_update
mv error_${y}* /datastore/GLOBALWATER/CommonData/EMDNA/PyGMETout_update
done


for (( y=2018; y<=2018; y++))
do
sbatch Plato_${y}.sh
done