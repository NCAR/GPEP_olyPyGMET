sbatch --array=2-39%38 Plato_readown.sh
sbatch --array=1-1%1 Plato_readown1.sh

for (( y=2001; y<=2018; y++))
do
for (( m=1; m<=12; m++ ))
do
if ((m<10)); then
sbatch Plato_"$y"0"$m".sh
else
sbatch Plato_"$y""$m".sh
fi
done
done


for (( y=2017; y<=2017; y++))
do
mv output_${y}* /datastore/GLOBALWATER/CommonData/EMDNA/PyGMETout_update
mv error_${y}* /datastore/GLOBALWATER/CommonData/EMDNA/PyGMETout_update
done

for (( y=2001; y<=2006; y++))
do
