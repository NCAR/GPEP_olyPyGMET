done

for (( y=2013; y<=2018; y++))
do
mv GWRLSBMA_merge/bmamerge_trange_${y}* /datastore/GLOBALWATER/CommonData/EMDNA_new/ReanalysisCorrMerge/GWRLSBMA_merge
done

for (( y=1980; y<=2018; y++))
do
sbatch Plato_prcp_${y}.sh
sbatch Plato_trange_${y}.sh
done

for y in {1980..2018..1}
do
sbatch Plato_reg_${y}.sh
done


for y in {1979..2018..1}
do
